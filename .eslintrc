{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "prettier"
  ],
  "plugins": [
    "jest-formatting"
  ],
  "rules": {
    "complexity": "off",
    "prefer-destructuring": "off",
    "no-warning-comments": "off",
    "dot-notation": [
      "error",
      {
        "allowKeywords": true
      }
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "consistent-return": "off",
    "curly": [
      "error"
    ],
    "jest-formatting/padding-around-all": "error"
  },
  "overrides": [
    {
      "files": [
        "*.spec.*"
      ],
      "rules": {
        "no-sparse-arrays": "off",
        "max-nested-callbacks": [
          "error",
          5
        ]
      }
    }
  ]
}
